source 'http://rubygems.org'

gem 'rails', '3.0.5'

# UUIDs
gem "uuid"

# JSON (much faster than the Rails default)
gem "json"

# HTML5 Boilerplate
gem "compass", ">= 0.10.6"
gem "haml"

# Solr
gem "rsolr"
gem "rsolr-ext"

# Paginate
gem "will_paginate"

# Markdown
gem "maruku"

# JQuery
gem "jquery-rails"

# Various output formats: MARC, XML, etc.
gem "marc"
gem "builder"
gem "rdf"
gem "rdf-raptor"
