- data = JSON.load(@task.result.file_contents(:original))

.row
  .col-md-8
    %h1== #{Dataset.model_name.human}: #{@dataset.name}
    %h4.text-muted= t('jobs.analysis.network.subheader', word: data['word'])

  .col-md-4.job-header-button
    = link_to t('jobs.analysis.network.download'), dataset_analysis_task_path(@dataset, @task, view: 'download', format: 'graphml'), class: 'btn btn-default'

.row
  .col-xs-12
    %hr

    .network_graph_nodes= data['d3_nodes'].to_json
    .network_graph_links= data['d3_links'].to_json

    .network_graph
      .network_graph_box
        %p ...
