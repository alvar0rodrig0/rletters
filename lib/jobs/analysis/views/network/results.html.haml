- data = JSON.load(@task.result.file_contents(:original))

.row
  .col-md-8
    %h1== #{Dataset.model_name.human}: #{@dataset.name}
    %h4.text-muted= t('jobs.analysis.network.subheader', word: data['word'])

  .col-md-4.job-header-button
    = link_to t('jobs.analysis.network.download'), dataset_task_path(@dataset, @task, view: 'download', format: 'graphml'), class: 'btn btn-default'

.row
  .col-xs-12
    %hr

    %script{ type: 'text/javascript' }
      == window.json_data = '#{@json_escaped}';
    .network_graph
      .network_graph_box
        %p ...
