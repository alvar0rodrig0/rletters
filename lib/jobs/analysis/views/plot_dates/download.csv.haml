- require 'csv'
- data = JSON.load(@task.result.file_contents(:original))
- dates = data['data']
- percent = data['percent']
!= CSV.generate do |csv|
  - csv << [ Document.human_attribute_name(:year), percent ? t('jobs.analysis.plot_dates.fraction_column') : t('jobs.analysis.plot_dates.number_column') ]
  - dates.each do |d|
    - csv << d
